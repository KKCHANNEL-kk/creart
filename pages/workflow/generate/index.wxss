.generate-page-container,
.generate-page-header {
  display: block;
  background: #f6f6f6;
  min-height: 100vh;
}

.styles-page-container .template-list-container {
  background: #f5f5f5 !important;
  margin-top: 16rpx;
}

.generate-page-container .form-item {
  margin-bottom: 24rpx;
  box-sizing: border-box;
}

.form-item-image {
  background-color: #fff;
  padding: 32rpx;
}

.form-item-image .label {
  padding-bottom: var(--td-spacer, 16rpx);
}